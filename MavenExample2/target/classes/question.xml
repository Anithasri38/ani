<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
      
     <bean id="questions" class="com.question.Question">
     
       <property name="questionId" value="1" />
       <property name="question" value="How are you" />
       <property name="answers1">
       <list>
        <value>fine</value>
        <value>good</value>
       </list>
       
       </property>
       <property name="answers2">
       <set>
         <value>nice</value>
         <value>excellent</value>
         </set>
       </property>
       <property name="answers3">
          <map>
            <entry key="1">
               <value>not bad</value>
            </entry>
            <entry key="2">
              <value>very good</value>
            </entry>
          </map>
       </property>
         
     </bean>
       
        
  </beans>      